<h1>Top Dive Spots in Los Angeles</h1>
<main>
    <div class="container">
        <div class="row">
            <div class="col s6">
                {{!-- {{#each topSpots}} --}}
                <h2>{{this.spot}}</h2>
                <div class="slider">
                    <ul class="slides">
                        {{#each this.images}}
                        <li>
                            <img src = {{this}}> 
                        </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
            <div class="col s6">
                <div class="card-panel">
                    {{this.body}}
                </div>
                {{!-- {{/each}} --}}
            </div>
        </div>
    </div>
</main>

<script>
    $(document).ready(function(){
      $('.slider').slider();
    });
</script>